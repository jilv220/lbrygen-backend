{
  "name": "daemon",
  "version": "1.0.0",
  "description": "api and daemon required for lbrygen-electron",
  "type": "module",
  "main": "api.js",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "apicache": "^1.6.3",
    "axios": "^0.26.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "node-fetch": "^3.2.4"
  },
  "scripts": {
    "build": "npx tsc",
    "start": "2>/dev/null 1>/dev/null ./lbrynet start --config=daemon_settings.yml & node dist/api.js",
    "dev": "concurrently \"2>/dev/null 1>/dev/null ./lbrynet start\" \"npx tsc --watch\" \"nodemon -q dist/api.js\""
  },
  "engines": {
    "node": "16.x"
  },
  "devDependencies": {
    "@types/apicache": "^1.6.1",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^18.6.4",
    "concurrently": "^7.3.0",
    "nodemon": "^2.0.19",
    "typescript": "^4.7.4"
  }
}
